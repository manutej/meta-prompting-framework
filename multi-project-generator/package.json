{
  "name": "@meta-prompting/multi-project-generator",
  "version": "0.1.0",
  "description": "Generate and orchestrate 100+ websites in parallel using MCP servers, visual builders, and agentic coding tools",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "mpg": "dist/cli/index.js",
    "site": "dist/cli/index.js"
  },
  "scripts": {
    "build": "tsc && tsc-alias",
    "dev": "tsx watch src/cli/index.ts",
    "start": "node dist/cli/index.js",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "lint": "eslint src --ext .ts",
    "typecheck": "tsc --noEmit",
    "clean": "rimraf dist",
    "mcp:start": "node dist/mcp/server.js",
    "mcp:dev": "tsx watch src/mcp/server.ts"
  },
  "keywords": [
    "multi-project",
    "site-generator",
    "mcp",
    "turborepo",
    "monorepo",
    "visual-builder",
    "builder.io",
    "plasmic",
    "claude-code",
    "agentic-coding",
    "parallel-generation"
  ],
  "author": "Meta-Prompting Framework",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/manutej/meta-prompting-framework.git",
    "directory": "multi-project-generator"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.27.0",
    "@modelcontextprotocol/sdk": "^1.0.0",
    "chalk": "^5.3.0",
    "commander": "^12.1.0",
    "consola": "^3.2.3",
    "defu": "^6.1.4",
    "dotenv": "^16.4.5",
    "execa": "^9.3.0",
    "fs-extra": "^11.2.0",
    "glob": "^10.4.2",
    "inquirer": "^9.2.23",
    "js-yaml": "^4.1.0",
    "listr2": "^8.2.3",
    "lodash-es": "^4.17.21",
    "nanoid": "^5.0.7",
    "ora": "^8.0.1",
    "p-limit": "^6.1.0",
    "p-queue": "^8.0.1",
    "pathe": "^1.1.2",
    "picocolors": "^1.0.1",
    "superjson": "^2.2.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/inquirer": "^9.0.7",
    "@types/js-yaml": "^4.0.9",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.14.10",
    "@typescript-eslint/eslint-plugin": "^7.16.0",
    "@typescript-eslint/parser": "^7.16.0",
    "eslint": "^8.57.0",
    "rimraf": "^5.0.7",
    "tsc-alias": "^1.8.10",
    "tsx": "^4.16.2",
    "typescript": "^5.5.3",
    "vitest": "^1.6.0"
  },
  "peerDependencies": {
    "turbo": "^2.0.0"
  },
  "peerDependenciesMeta": {
    "turbo": {
      "optional": true
    }
  }
}
