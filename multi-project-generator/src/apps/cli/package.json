{
  "name": "@mpg/cli",
  "version": "0.1.0",
  "private": true,
  "description": "CLI for Multi-Project Generator",
  "type": "module",
  "bin": {
    "mpg": "./dist/index.js"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "start": "node dist/index.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@commander-js/extra-typings": "^14.0.0",
    "@mpg/core": "workspace:*",
    "@mpg/shared": "workspace:*",
    "chalk": "^5.3.0",
    "commander": "^14.0.2",
    "ora": "^8.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.3.0",
    "vitest": "^2.0.0"
  }
}
