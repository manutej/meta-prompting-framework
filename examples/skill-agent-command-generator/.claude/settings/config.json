  "composition_rules": {
    "skill_dependencies": {
      "result-type": ["option-type"],
      "pipeline": ["result-type"],
      "effect-isolation": ["pipeline"],
      "context-reader": ["effect-isolation"],
      "lazy-stream": ["context-reader"],
